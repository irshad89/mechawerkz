<% provide(:title, 'Document Binder Details') %>
<%- model_class = DocumentBinder -%>
<div class="page-header">
   <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<p id="notice"><%= notice %></p>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:code) %>:</strong></dt>
  <dd><%= @document_binder.code %></dd>



  <dt><strong><%= model_class.human_attribute_name(:description) %>:</strong></dt>
  <dd><%= @document_binder.description %></dd>



  <dt><strong><%= model_class.human_attribute_name(:volume) %>:</strong></dt>
  <dd><%= @document_binder.volume %></dd>



  <dt><strong><%= model_class.human_attribute_name(:contact_company) %>:</strong></dt>
  <dd><%= @document_binder.contact_company_id %></dd>



  <dt><strong><%= model_class.human_attribute_name(:owned_by) %>:</strong></dt>
  <dd><%= @document_binder.owned_by %><dd>



  <dt><strong><%= model_class.human_attribute_name(:location) %>:</strong></dt>
  <dd><%= @document_binder.location_id %></dd>
</dl>


<%= link_to 'Edit', edit_document_binder_path(@document_binder) %> |
<%= link_to 'Back', document_binders_path %>
