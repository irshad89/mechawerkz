<% provide(:title, 'Company Details') %>
<%- model_class = ContactCompany -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<p id="notice"><%= notice %></p>


<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:name) %>:</strong></dt>
  <dd><%= @contact_company.name %></dd>

	<dt><strong><%= model_class.human_attribute_name(:address) %>:</strong></dt>
  <dd><%= @contact_company.address %></dd>


<dt><strong><%= model_class.human_attribute_name(:postcode) %>:</strong></dt>
<dd> <%= @contact_company.postcode %></dd>



<dt>  <strong><%= model_class.human_attribute_name(:state) %>:</strong></dt>
  <dd><%= @contact_company.state %></dd>
</dl>


<%= link_to 'Edit', edit_contact_company_path(@contact_company) %> |
<%= link_to 'Back', contact_companies_path %>
